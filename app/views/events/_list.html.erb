<div class="row">
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"><%= list.title %></span>
        <p><%= '候補日 ' + list.min_day.strftime('%Y/%m/%d') + '〜' + list.max_day.strftime('%Y/%m/%d') %></p>
        <p><%= list.detail %></p>
      </div>
      <div class="card-action">
        <%= link_to('イベント詳細', "/events/#{list.id}", :action => 'show') %>
        <%= link_to('イベント編集', "/events/edit/#{list.id}", :action => 'edit') %>
      </div>
    </div>
  </div>
</div>